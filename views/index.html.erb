  <script type="text/x-handlebars">
    <div id="container">
      <a href="index.html" id="title"><img src="images/logoTitle.jpg" alt="i5labs" /></a>
      <h1><span class="displace">i5labs</span></h1>

      <div id="navigation">
        <a href="about.html" id="rolloverAbout" class="about"><span class="displace">About</span></a>
        <a href="sfgovPortfolio.html" id="rolloverPortfolio" class="portfolio"><span class="displace">Portfolio</span></a>
        <a href="http://blog.i5labs.com" id="rolloverBlog" class="blog"><span class="displace">Blog</span></a>
        <a href="cdnIntroduction.html" id="rolloverCdn" class="cdn"><span class="displace">CDN</span></a>
        <a href="contact.html" id="rolloverContact" class="contact"><span class="displace">Contact</span></a>
      </div>

    <div id='cdn'>
      <div id='cdnNavigation'>
      {{#view App.tabView}}
        <div class='flourish'>
          <div class='flourishTop'></div>
          <div class='flourishMiddle'></div>
          <div class='flourishBottom'></div>
        </div>
        {{#linkTo "index"}} <img alt='Introduction' src='images/introductionTab.png' />{{/linkTo}}
        {{outlet "intro"}}
      {{/view}}

      {{#view App.tabView}}
        <div class='flourish'>
          <div class='flourishTop'></div>
          <div class='flourishMiddle'></div>
          <div class='flourishBottom'></div>
        </div>
          {{#linkTo "how"}}<img alt='How it Works' src='images/howitworksTab.png' />{{/linkTo}}
          {{outlet "how"}}
      {{/view}}

      {{#view App.tabView}}
        <div class='flourish'>
          <div class='flourishTop'></div>
          <div class='flourishMiddle'></div>
          <div class='flourishBottom'></div>
        </div>
          {{#linkTo "advantage"}}<img alt='The i5labs Advantage' src='images/advantageTab.png' />{{/linkTo}}}
          {{outlet "advantage"}}
      {{/view}}
    </div>

    <div id='cdnContent'>


      <div class='flourish'>
        <div class='flourishTop'></div>
        <div class='flourishMiddle'></div>
        <div class='flourishBottom'></div>
      </div>
      <img alt='30 Day Free Trial' src='images/freetrialTab.png' />
    {{#if planNotPicked}}
      <div id='cdnPlanChooser'>
        <p>Choose a plan and start streaming today:</p>
        <div id='cdnPlanOption'>
      {{#each model}}
        <img  {{action 'showPlan' this}} alt='50GB transfer / 1GB Storage / $100 per month' {{bindAttr src="image"}} />
      {{/each}}
          </a>
          <p>*all plans include 30 day free trial and $100 setup fee</p>
        </div>
      </div>
    {{else}}
      <div id="plan">
        <span class="pink" {{action 'showAllPlans'}}>All Plans</span>
        <span class="grey"> {{selectedPlan.transfer}} GB transfer plan</span>
        <br>

        <div class='planDetail'>
          <p class='size'>{{selectedPlan.transfer}}</p>
          <p class='unit'>GB</p>
          <p class='extra'>(${{selectedPlan.extratransfer}} / additional gigabyte)</p>
          <p class='use'>monthly transfer</p>
        </div>
        <div class='planDetail'>
          <p class='size'>{{selectedPlan.storage}}</p>
          <p class='unit'>GB</p>
          <p class='extra'>(${{selectedPlan.extrastorage}} / additional gigabyte)</p>
          <p class='use'>storage</p>
        </div>
        <div class='planDetail'>
          <p class='size'>30</p>
          <p class='unit'>Day</p>
          <p class='use'>free trial</p>
        </div>
        <p class='frequency'>per month</p>
        <p class='price'>${{selectedPlan.price}}</p>
      </div>
      <div id='cdnForm'>
        <form>
          <div class='fields'>
            <label for='contact_name'>Name</label>
            {{view Ember.TextField valueBinding="cdnSignup.name"}}
            <!--<input class='required' id='contact_name' {{bindAttr valueBinding=cdnSignUp.name}} title='Please enter your full name' type='text' />
            <label for='contact_phone'>Phone</label>
            <input class='phone' id='contact_phone' name='lead[phone]' title='Please enter a valid phone number.' type='text' />
            <label for='contact_email'>Email</label>
            <input class='required email' id='contact_email' name='lead[email]' title='Please enter a valid email' type='text' />
            <label for='contact_company'>Company</label>
            <input id='contact_company' name='lead[company_name]' title='Please enter your company name' type='text' />
            <input name='tag_1' type='hidden' value='Google Adwords' />
            <input name='lead[custom_label_1]' type='hidden' value='Plan' />
            <input name='lead[custom_value_1]' type='hidden' value='$100 per month' />-->
          </div>
          <div class='cdnSignUpButton'>
            <input alt='submit' name='commit' src='images/cdnSignUpButton.gif' type='image' />
          </div>
        </form>
      </div>
    {{/if}}
    </div>
    <div id="footer">
       <p>i5labs &#169; | 450 Geary Street, Suite 400, San Francisco,  CA 94102 | 415.929.9999 | <a href="mailto:bizdev@i5labs.com">bizdev@i5labs.com</a></p>
    </div>
